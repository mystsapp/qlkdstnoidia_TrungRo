@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    /* Darker background on mouse-over */
    .btn:hover {
        background-color: RoyalBlue;
    }
</style>
@*@if (TempData["AlertType"] != null)
    {
        Html.RenderPartial("ErrorMessageView");
        //reset
        TempData["AlertType"] = null;
    }*@

<div class="wthree_general graph-form agile_info_shadow ">
    <h3 class="w3_inner_tittle two">THÊM KHÁCH TOUR TỪ FILE EXCEL</h3>

    <div class="grid-1">
        <div class="form-body">

            @using (Html.BeginForm("ImportDmKhach", "Room", FormMethod.Post, new {@class = "form-horizontal", @id = "frmImportExcel", enctype = "multipart/form-data" }))
            {
                <div class="form-group">
                    <label   class="col-sm-2 control-label">   Download file mẫu:</label>
                    <div class="col-sm-10">
                        <a href="~/ExcelFiles/DmKhach/danh muc khach tour mau.xlsx">Danh mục khách Excel mẫu</a>
                    </div>

                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label">   Chọn file:</label>
                    <div class="col-sm-10">                        
                        <input type="file" id="dataFile" name="uploadExcel" class="form-control" />
                    </div>
                </div>
               
                    <div class="col-sm-offset-2">
                        <button name="submit" class="btn btn-primary" id="btnSave"><i class="fa fa-save"></i>Lưu</button>
                    </div>
                             

                    }



                </div>
    </div>
</div>



